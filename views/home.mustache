<!doctype html>
<head>
</head>

<body>
  <h1> Hello there {{ username! }} </h1>
  <h2> You've logged in {{ logintimes }} times so far. </h2>

  <h3> Messages from your friends: </h3>
  <ul>
      {{ #messages }}
      <li>
        <span>"{{ username.message}}" -- {{ username }}</span>
      </li>
      {{ /messages }}
  </ul>

  <form method="post" action="/logout">
  <input type="submit" value="Log out">
  </form>
</body>

</html>
